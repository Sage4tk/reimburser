{
  "name": "generate-pdf-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda function for generating expense receipt PDFs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --external:@aws-sdk/*",
    "package": "npm run build && cd dist && zip -r ../function.zip ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.679.0",
    "@supabase/supabase-js": "^2.39.0",
    "jspdf": "^2.5.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.10.0",
    "esbuild": "^0.19.8",
    "typescript": "^5.3.3"
  }
}
